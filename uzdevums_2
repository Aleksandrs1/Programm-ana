a1 = 'teksts'
a2 = 'kraz'
print(a1)
print(a2)
print(a1,a2)
print(a1+a2)


print(a1[0:2], a2[3:5])
a5 = 12345
a6 = 12345
print(a5 + a6)
print(a5)
print(a6)
print(a5*a6)
a3 = 1.34
a4 = 1.89
print(a3 + a4)
print(a3+a4, a3 + a4)


print(a3*a4)
print("teksts {}".format (a1))
list = [a1,a2,a3]
print(list)
print(list,'\n')
print("Lenght list:", len(list),'\n')
print(list + ["Korobka"])
list[0] = 'Krrrrrr'
print(list[0],'\n')